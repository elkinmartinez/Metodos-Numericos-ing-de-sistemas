# metodo para manejar solo tres digitos
def digitos(cadena):
    b = str(cadena)
    numero = b[:3]
    if '.' or '-'in b:
        numero = b[:4]
    if '-' and '.' in b:
        numero = b[:5]
    return float(numero)

